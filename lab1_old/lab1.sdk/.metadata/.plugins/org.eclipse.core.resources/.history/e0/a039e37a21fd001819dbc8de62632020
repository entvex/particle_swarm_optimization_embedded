#ifndef SRC_STATEMACHINE_STATE_H_
#define SRC_STATEMACHINE_STATE_H_

#include "Context.h"

class State {
public:
	State();
	virtual void GoMaxima(Context*);
	virtual void GoMinima(Context*);
	virtual void ReturnResult(Context*);

	virtual ~State();
};

#endif /* SRC_STATEMACHINE_STATE_H_ */
