#include "QueueScheduler.h"

QueueScheduler::QueueScheduler() {
	// TODO Auto-generated constructor stub

}

QueueScheduler::~QueueScheduler() {

}

void QueueScheduler::put(String s)
{
	sim_queue.push(s);
	empty.notify_one();
}

String QueueScheduler::take()
{
	String sim = sim_queue.front();
	sim_queue.pop();
	return sim;
}
